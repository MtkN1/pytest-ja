#: ../../announce/release-2.2.2.rst:26
msgid "Changes between 2.2.1 and 2.2.2"
msgstr ""

#: ../../announce/release-2.2.2.rst:28
msgid "fix issue101: wrong args to unittest.TestCase test function now produce better output"
msgstr ""

#: ../../announce/release-2.2.2.rst:30
msgid "fix issue102: report more useful errors and hints for when a test directory was renamed and some pyc/__pycache__ remain"
msgstr ""

#: ../../announce/release-2.2.2.rst:32
msgid "fix issue106: allow parametrize to be applied multiple times e.g. from module, class and at function level."
msgstr ""

#: ../../announce/release-2.2.2.rst:34
msgid "fix issue107: actually perform session scope finalization"
msgstr ""

#: ../../announce/release-2.2.2.rst:35
msgid "don't check in parametrize if indirect parameters are funcarg names"
msgstr ""

#: ../../announce/release-2.2.2.rst:36
msgid "add chdir method to monkeypatch funcarg"
msgstr ""

#: ../../announce/release-2.2.2.rst:37
msgid "fix crash resulting from calling monkeypatch undo a second time"
msgstr ""

#: ../../announce/release-2.2.2.rst:38
msgid "fix issue115: make --collectonly robust against early failure (missing files/directories)"
msgstr ""

#: ../../announce/release-2.2.2.rst:40
msgid "\"-qq --collectonly\" now shows only files and the number of tests in them"
msgstr ""

#: ../../announce/release-2.2.2.rst:41
msgid "\"-q --collectonly\" now shows test ids"
msgstr ""

#: ../../announce/release-2.2.2.rst:42
msgid "allow adding of attributes to test reports such that it also works with distributed testing (no upgrade of pytest-xdist needed)"
msgstr ""

