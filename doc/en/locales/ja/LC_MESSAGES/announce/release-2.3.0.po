#: ../../announce/release-2.3.0.rst:56
msgid "Changes between 2.2.4 and 2.3.0"
msgstr ""

#: ../../announce/release-2.3.0.rst:58
msgid "fix issue202 - better automatic names for parametrized test functions"
msgstr ""

#: ../../announce/release-2.3.0.rst:59
msgid "fix issue139 - introduce @pytest.fixture which allows direct scoping and parametrization of funcarg factories.  Introduce new @pytest.setup marker to allow the writing of setup functions which accept funcargs."
msgstr ""

#: ../../announce/release-2.3.0.rst:62
msgid "fix issue198 - conftest fixtures were not found on windows32 in some circumstances with nested directory structures due to path manipulation issues"
msgstr ""

#: ../../announce/release-2.3.0.rst:64
msgid "fix issue193 skip test functions with were parametrized with empty parameter sets"
msgstr ""

#: ../../announce/release-2.3.0.rst:66
msgid "fix python3.3 compat, mostly reporting bits that previously depended on dict ordering"
msgstr ""

#: ../../announce/release-2.3.0.rst:68
msgid "introduce re-ordering of tests by resource and parametrization setup which takes precedence to the usual file-ordering"
msgstr ""

#: ../../announce/release-2.3.0.rst:70
msgid "fix issue185 monkeypatching time.time does not cause pytest to fail"
msgstr ""

#: ../../announce/release-2.3.0.rst:71
msgid "fix issue172 duplicate call of pytest.setup-decoratored setup_module functions"
msgstr ""

#: ../../announce/release-2.3.0.rst:73
msgid "fix junitxml=path construction so that if tests change the current working directory and the path is a relative path it is constructed correctly from the original current working dir."
msgstr ""

#: ../../announce/release-2.3.0.rst:76
msgid "fix \"python setup.py test\" example to cause a proper \"errno\" return"
msgstr ""

#: ../../announce/release-2.3.0.rst:77
msgid "fix issue165 - fix broken doc links and mention stackoverflow for FAQ"
msgstr ""

#: ../../announce/release-2.3.0.rst:78
msgid "catch unicode-issues when writing failure representations to terminal to prevent the whole session from crashing"
msgstr ""

#: ../../announce/release-2.3.0.rst:80
msgid "fix xfail/skip confusion: a skip-mark or an imperative pytest.skip will now take precedence before xfail-markers because we can't determine xfail/xpass status in case of a skip. see also: http://stackoverflow.com/questions/11105828/in-py-test-when-i-explicitly-skip-a-test-that-is-marked-as-xfail-how-can-i-get"
msgstr ""

#: ../../announce/release-2.3.0.rst:85
msgid "always report installed 3rd party plugins in the header of a test run"
msgstr ""

#: ../../announce/release-2.3.0.rst:87
msgid "fix issue160: a failing setup of an xfail-marked tests should be reported as xfail (not xpass)"
msgstr ""

#: ../../announce/release-2.3.0.rst:90
msgid "fix issue128: show captured output when capsys/capfd are used"
msgstr ""

#: ../../announce/release-2.3.0.rst:92
msgid "fix issue179: properly show the dependency chain of factories"
msgstr ""

#: ../../announce/release-2.3.0.rst:94
msgid "pluginmanager.register(...) now raises ValueError if the plugin has been already registered or the name is taken"
msgstr ""

#: ../../announce/release-2.3.0.rst:97
msgid "fix issue159: improve https://docs.pytest.org/en/6.0.1/faq.html especially with respect to the \"magic\" history, also mention pytest-django, trial and unittest integration."
msgstr ""

#: ../../announce/release-2.3.0.rst:101
msgid "make request.keywords and node.keywords writable.  All descendant collection nodes will see keyword values.  Keywords are dictionaries containing markers and other info."
msgstr ""

#: ../../announce/release-2.3.0.rst:105
msgid "fix issue 178: xml binary escapes are now wrapped in py.xml.raw"
msgstr ""

#: ../../announce/release-2.3.0.rst:107
msgid "fix issue 176: correctly catch the builtin AssertionError even when we replaced AssertionError with a subclass on the python level"
msgstr ""

#: ../../announce/release-2.3.0.rst:111
msgid "factory discovery no longer fails with magic global callables that provide no sane __code__ object (mock.call for example)"
msgstr ""

#: ../../announce/release-2.3.0.rst:114
msgid "fix issue 182: testdir.inprocess_run now considers passed plugins"
msgstr ""

#: ../../announce/release-2.3.0.rst:116
msgid "fix issue 188: ensure sys.exc_info is clear on python2"
